#include <iostream>
using namespace std;

int main() {
    int rows;
    cout << "Enter the number of rows: ";
    cin >> rows;

    for (int i = 1; i <= rows; ++i) {
        // Print the number `i` exactly `i` times
        for (int j = 1; j <= i; ++j) {
            cout << i;
        }
        cout << endl; // Move to the next line
    }

    return 0;
}
